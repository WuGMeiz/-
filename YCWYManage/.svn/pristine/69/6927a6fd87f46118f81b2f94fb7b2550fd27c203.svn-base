@charset "UTF-8";
*{margin: 0; padding: 0; box-sizing: border-box;}
body{font-family: "微软雅黑", Arial, sans-serif; font-size: 16px; margin: 0 auto; background: #f5f5f5;}
ul{list-style: none;}
a{color: inherit; text-decoration: none;}
.fl{float: left;}
.fr{float: right;}
.clear:after{content: ""; display: block; clear: both;}
img{pointer-events: none;}
table{border-collapse:collapse;}
.hidden{display: none !important;}
.none{position: absolute; opacity: 0; z-index: -100; width: 0; height: 0;}
/*弹出层*/
.cover{
    display: none;
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 500;
    background: url("../image/opacity.png") repeat;
}
.coverContent{
    width: 400px;
    height: 240px;
    background: #cee8fc;
    border-radius: 5px;
    padding: 10px 15px;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 10%;
    margin: auto;
}
.timerTip{
    text-align: right;
    font-size: 14px;
    line-height: 32px;
    height: 32px;
    color: #999999;
}
.orderState{
    font-size: 16px;
    line-height: 32px;
    text-align: center;
    font-weight: 900;
    color: #333333;
    margin: 25px auto 15px;
}
.coverContent .tip{
    text-align: center;
}
.tip{
    font-size: 12px;
    line-height: 30px;
    color: #fb3043;
}
.red{
    color: #fb3043;
}
.centerBtn{
    display: block;
    width: 160px;
    height: 36px;
    margin: 15px auto;
    border-radius: 4px;
    border: none;
    outline: none;
    color: #f2f2f2;
    background: #67b0ef;
    font-size: 15px;
    letter-spacing: 5px;
    text-indent: 5px;
}
.centerBtn:disabled{
    color: #aaaaaa;
    background: #348ad4;
}